{
  "name": "curated-pull-out-sources-ph",
  "version": "1.0.1",
  "description": "Curated pull out sources",
  "main": "dist/index.js",
  "keywords": ["crawler", "for closed properties"],
  "types": "index.d.ts",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}x]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.28",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.22.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "4.0.2"
  },
  "author": "d4rth",
  "license": "ISC",
  "engines": {
    "node": ">=12",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "class-transformer": "^0.4.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "puppeteer": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  }
}
